{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
     "typeRoots": ["./typings", "./node_modules/@types"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "typings/**/*.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
